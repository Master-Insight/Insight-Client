{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@ico/*": ["src/components/icons/*"],
      "@config/*": ["config/*"],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}